    <link href="style.css" rel="stylesheet" />

<title>Mojobs</title>
<body>
  <h1>Job Postings</h1>
  <ul>
    {{#each jobs}}
      <li>
        <h2>{{title}}</h2>
        <p>{{company}}</p>
        <p>{{location}}</p>
        <p>{{description}}</p>
      </li>
    {{/each}}
  </ul>
</body>
</html>
